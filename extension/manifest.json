{
  "name": "Chrome Speed Enhancer",
  "description": "Makes your chrome browser run extra fast",
  "version": "0.1.0",
  "manifest_version": 2,
  "icons": {
    "16": "./images/get_started16.png",
    "32": "./images/get_started32.png",
    "48": "./images/get_started48.png",
    "128": "./images/get_started128.png"
  },
  "background": {
    "scripts": [
      "./injector.js"
    ]
  },
  "options_page": "./options.html",
  "permissions": [
    "<all_urls>",
    "tabs"
  ]
}